<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" prefWidth="450.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints maxWidth="250.0" minWidth="10.0" />
      <ColumnConstraints maxWidth="200.0" minWidth="10.0" />
      <ColumnConstraints maxWidth="250.0" minWidth="10.0" />
      <ColumnConstraints maxWidth="200.0" minWidth="10.0" />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>

      <TextField fx:id="Display" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.columnSpan="4">
         <font>
            <Font size="38.0" />
         </font>
         <GridPane.margin>
            <Insets top="10.0" />
         </GridPane.margin></TextField>

      <Button fx:id="One" mnemonicParsing="false" onAction="#actionOne" prefHeight="100.0" prefWidth="100.0" text="1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="Two" mnemonicParsing="false" onAction="#actionTwo" prefHeight="100.0" prefWidth="100.0" text="2" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Button fx:id="Three" mnemonicParsing="false" onAction="#actionThree" prefHeight="100.0" prefWidth="100.0" text="3" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <Button fx:id="Divide" mnemonicParsing="false" onAction="#actionDivide" prefHeight="100.0" prefWidth="100.0" text="/" GridPane.columnIndex="4" GridPane.rowIndex="1" />

      <Button fx:id="Four" mnemonicParsing="false" onAction="#actionFour" prefHeight="100.0" prefWidth="100.0" text="4" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button fx:id="Five" mnemonicParsing="false" onAction="#actionFive" prefHeight="100.0" prefWidth="100.0" text="5" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <Button fx:id="Six" mnemonicParsing="false" onAction="#actionSix" prefHeight="100.0" prefWidth="100.0" text="6" GridPane.columnIndex="3" GridPane.rowIndex="2" />
      <Button fx:id="Multiply" mnemonicParsing="false" onAction="#actionMultiply" prefHeight="100.0" prefWidth="100.0" text="X" GridPane.columnIndex="4" GridPane.rowIndex="2" />

      <Button fx:id="Seven" mnemonicParsing="false" onAction="#actionSeven" prefHeight="100.0" prefWidth="100.0" text="7" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Button fx:id="Eight" mnemonicParsing="false" onAction="#actionEight" prefHeight="100.0" prefWidth="100.0" text="8" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <Button fx:id="Nine" mnemonicParsing="false" onAction="#actionNine" prefHeight="100.0" prefWidth="100.0" text="9" GridPane.columnIndex="3" GridPane.rowIndex="3" />
      <Button fx:id="Minus" mnemonicParsing="false" onAction="#actionMinus" prefHeight="100.0" prefWidth="100.0" text="-" GridPane.columnIndex="4" GridPane.rowIndex="3" />

      <Button fx:id="Zero" mnemonicParsing="false" onAction="#actionZero" prefHeight="100.0" prefWidth="100.0" text="0" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <Button fx:id="Plus" mnemonicParsing="false" onAction="#actionPlus" prefHeight="100.0" prefWidth="100.0" text="+" GridPane.columnIndex="4" GridPane.rowIndex="4" />
      <Button fx:id="Point" mnemonicParsing="false" onAction="#actionPoint" prefHeight="100.0" prefWidth="100.0" text="." GridPane.columnIndex="3" GridPane.rowIndex="4" />


      <Button fx:id="Store" mnemonicParsing="false" onAction="#actionStore" prefHeight="100.0" prefWidth="100.0" text="RS" GridPane.columnIndex="1" GridPane.rowIndex="5" />
       <Button fx:id="Recall" mnemonicParsing="false" onAction="#actionRecall" prefHeight="100.0" prefWidth="100.0" text="RC" GridPane.columnIndex="2" GridPane.rowIndex="5" />
       <Button fx:id="Enter" mnemonicParsing="false" onAction="#actionEnter" prefHeight="100.0" prefWidth="100.0" text="Enter" GridPane.columnIndex="3" GridPane.columnSpan="4" GridPane.rowIndex="5" />
      <Group GridPane.columnIndex="1" />


   </children>
   <padding>
      <Insets left="10.0" />
   </padding>
</GridPane>
